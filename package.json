  },
  "homepage": "https://github.com/RenGeJiaYou/Money-Management-System-by-Vue-Node.js#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "concurrently": "^7.2.2",
    "express": "^4.18.1",
    "gravatar": "^1.8.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.4.2",
    "nodemon": "^2.0.18",
    "passport": "^0.6.0",
